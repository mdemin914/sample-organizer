{
  "name": "sample-organizer",
  "version": "1.0.0",
  "description": "A Mac OS app for organizing musical samples on your hard drive",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:main\" \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "webpack --config webpack.config.js --watch --mode development",
    "dev:main": "tsc -p tsconfig.json --watch --outDir dist",
    "dev:electron": "wait-on dist/main.js dist/renderer.js && NODE_ENV=development electronmon .",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.json --outDir dist",
    "build:renderer": "webpack --config webpack.config.js --mode production",
    "start": "electron .",
    "dist": "electron-builder --mac --x64",
    "test": "jest"
  },
  "keywords": [
    "electron",
    "music",
    "samples",
    "organizer"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@mui/x-data-grid": "^8.11.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-window": "^1.8.8",
    "concurrently": "^8.0.0",
    "electron-builder": "^24.6.0",
    "electron-devtools-installer": "^4.0.0",
    "electronmon": "^1.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.3.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^7.3.2",
    "@mui/x-tree-view": "^6.0.0-alpha.3",
    "electron": "^28.3.3",
    "fs-extra": "^11.1.1",
    "openai": "^4.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.7"
  }
}
